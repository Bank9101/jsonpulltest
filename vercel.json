{
    "version": 2,
    "builds": [
        {
            "src": "jsonpull/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb"
            }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/jsonpull/wsgi.py"
        },
        {
            "src": "/(.*)",
            "dest": "/jsonpull/wsgi.py"
        }
    ],
    "env": {
        "PYTHONPATH": "."
    }
}